.ci-pagination-button{
    display: flex;
    justify-content: space-between;
    text-align: center
}
.ci-text-wrapper{
    line-height: 92rpx;
    margin: 0 auto
}
.ci-current-text{
    color: #108ee9;
}
.ci-pagination-pointer{
    display: flex;
    flex-direction: row;
    justify-content: center
}
.ci-pointer{
    width: 30rpx;
    height: 30rpx;
    border-radius: 30rpx;
    background: #ccc;
    margin-right: 20rpx
}

.ci-pointer-isChecked{
    background: #000
}